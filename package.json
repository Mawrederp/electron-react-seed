{
    "name": "electron-react-seed",
    "version": "0.0.1",
    "description": "electron react mobx seed",
    "main": "src/desktop/main.js",
    "scripts": {
        "electron": "electron .",
        "prettier-all": "prettier --config .prettierrc --write 'src/**/*.js'",
        "prettier-changed": "git diff --name-only HEAD | grep \".*\\.js$\" | xargs prettier --config .prettierrc --write",
        "test": "mocha --require source-map-support --require babel-register --require babel-polyfill --require init.mocha.js --check-leaks --timeout 3000 'src/**/*.spec.js'",
        "coverage": "istanbul cover -x 'src/**/*.spec.js' ./node_modules/.bin/_mocha -- --require source-map-support --require babel-register --require babel-polyfill --require init.mocha.js --check-leaks --timeout 3000 'src/**/*.spec.js' && open coverage/lcov-report/index.html",
        "complexity": "plato -r --eslint .eslintrc --exclude .spec.js -d report src && open report/index.html",
        "linter": "eslint src",
        "precommit": "lint-staged && npm run build && git add dist/ && npm run linter && npm test",
        "build": "webpack --config webpack.babel.js",
        "dev": "webpack-dev-server --config webpack.babel.js --hot"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/uhlryk/wallet.git"
    },
    "keywords": [
        "react",
        "mobx",
        "sass",
        "electron"
    ],
    "author": "Krzysztof Sztompka",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/uhlryk/wallet/issues"
    },
    "homepage": "https://github.com/uhlryk/wallet#readme",
    "devDependencies": {
        "babel-cli": "^6.24.1",
        "babel-core": "^6.25.0",
        "babel-eslint": "^8.0.0",
        "babel-loader": "^7.1.1",
        "babel-plugin-transform-decorators-legacy": "^1.3.4",
        "babel-polyfill": "^6.23.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-es2016": "^6.24.1",
        "babel-preset-es2017": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "babel-preset-stage-1": "^6.24.1",
        "babel-register": "^6.24.1",
        "chai": "^4.0.2",
        "clean-webpack-plugin": "^0.1.18",
        "dirty-chai": "^2.0.1",
        "eslint": "^4.7.2",
        "eslint-config-prettier": "^2.9.0",
        "eslint-config-standard": "^10.2.1",
        "eslint-plugin-import": "^2.7.0",
        "eslint-plugin-mocha": "^4.11.0",
        "eslint-plugin-node": "^5.1.1",
        "eslint-plugin-promise": "^3.5.0",
        "eslint-plugin-react": "^7.6.1",
        "eslint-plugin-standard": "^3.0.1",
        "html-webpack-plugin": "^2.30.1",
        "husky": "^0.14.3",
        "istanbul": "^1.1.0-alpha.1",
        "lint-staged": "^6.0.0",
        "mocha": "^3.4.2",
        "mongo-mock": "^2.4.0",
        "plato": "^1.7.0",
        "prettier": "^1.6.1",
        "react-hot-loader": "^4.0.0-beta.21",
        "sinon": "^2.3.6",
        "sinon-chai": "^2.13.0",
        "supertest": "^3.0.0",
        "webpack": "^3.10.0",
        "webpack-dev-server": "^2.11.1",
        "webpack2-externals-plugin": "^1.0.0"
    },
    "dependencies": {
        "electron": "^1.8.1",
        "mobx": "^3.4.1",
        "mobx-react": "^4.3.5",
        "mobx-react-devtools": "^4.2.15",
        "react": "^16.2.0",
        "react-dom": "^16.2.0"
    },
    "lint-staged": {
        "*.js": [
            "prettier --config .prettierrc --write",
            "git add"
        ]
    }
}
