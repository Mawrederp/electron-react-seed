{
  "presets": [
    ["env", {
      "targets": { "node": 7 },
      "useBuiltIns": true
    }],
    "stage-0",
    "react"
  ],
  "plugins": ["add-module-exports"],
  "env": {
    "production": {
      "presets": ["react-optimize"],
      "plugins": [
          "transform-decorators-legacy",
          "transform-decorators",
          "transform-class-properties",
          "transform-es2015-classes",
          "dev-expression"
      ]
    },
    "development": {
      "plugins": [
          "transform-decorators-legacy",
          "transform-decorators",
          "transform-class-properties",
          "transform-es2015-classes"

      ]
    }
  }
}
